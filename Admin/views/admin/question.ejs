                                        <!-- main-menu end -->
                                    </div>
                                </nav>
                            </div>
                            <!-- main-navigation end -->
                        </div>
                        <!-- header-second end -->

                    </div>
                </div>
            </div>
        </header>
        <!-- header end -->
    </div>
    <!-- header-container end -->

    <!-- section start -->
    <!-- ================ -->

    <!-- section end -->

    <!-- main-container start -->
    <!-- ================ -->
    <div class="main-container" id = "user_main">
        <div class="container">
            <div class="row">
                <div class="main col-md-12">
                    <!-- pills start -->
                    <!-- ================ -->
                    <!-- Nav tabs -->
                    <ul class="nav nav-pills" role="tablist">
                        <li class="nav-item"><a class="nav-link" id="downSort" href="#pill-1" role="tab" data-toggle="tab" title="down_sort"><i class="fa fa-sort-alpha-desc"></i> 내림차순</a></li>
                        <li class="nav-item"><a class="nav-link" id="upSort" href="#pill-2" role="tab" data-toggle="tab" title="up_sort"><i class=" fa fa-sort-alpha-asc"></i> 오름차순</a></li>
                    </ul>

                    <!-- Tab panes -->
                    <div class="tab-content clear-style">
                        <div class="tab-pane active" id="pill-1">
                            <div id="down_list">
                            </div>
                        </div>
                        <div class="tab-pane" id="pill-2">
                            <div id="up_list">
                            </div>
                        </div>
                    </div>
                    <!-- pills end -->
                </div>
                <div class="main col-md-12">
                    <table class="table table-bordered table-hover" style="table-layout: fixed">
                        <thead>
                        <tr>
                            <th class="index">번호</th>
                            <th class="phone">연락처</th>
                            <th class="title">제목</th>
                            <th class="content">내용</th>
                            <th class="click">조회수</th>
                            <th class="date">등록일</th>
                            <th class="answer">답변여부</th>
                        </tr>
                        </thead>
                        <tbody>

                        <% for (var indexI = 0; indexI < questionList.length; indexI++) {%>
                        <tr>
                            <td><%= indexI+1 %></td>
                            <td><%= questionList[indexI].userId %></td>
                            <td><%= questionList[indexI].title %></td>
                            <td><%= questionList[indexI].content %></td>
                            <td><%= questionList[indexI].clickNumber %></td>
                            <td><%= moment(questionList[indexI].createdAt).format( 'YYYY-MM-DD HH:mm')  %></td>
                            <td><%= questionList[indexI].answerStatus %></td>
                        </tr>
                        <% } %>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>

    <!-- main-container end -->
    <!-- main-container end -->
