<script src="/template/plugins/isotope/imagesloaded.pkgd.min.js"></script>
<script src="/template/plugins/isotope/isotope.pkgd.min.js"></script>
<!-- Magnific Popup javascript -->
<script src="/template/plugins/magnific-popup/jquery.magnific-popup.min.js"></script>
<!-- Appear javascript -->
<script src="/template/plugins/waypoints/jquery.waypoints.min.js"></script>
<script src="/template/plugins/waypoints/sticky.min.js"></script>
<!-- Count To javascript -->
<script src="/template/plugins/countTo/jquery.countTo.js"></script>
<!-- Slick carousel javascript -->
<script src="/template/plugins/slick/slick.min.js"></script>
<!-- Pace javascript -->
<script src="/template/plugins/pace/pace.min.js"></script>
<!-- Initialization of Plugins -->


<script src="/template/js//template.js"></script>
<!-- Custom Scripts -->
<script src="/template/js/custom.js"></script>
<script src="/util/AjaxUtil.js"></script>
<script src="https://cdn.jsdelivr.net/bxslider/4.2.12/jquery.bxslider.min.js"></script>
<script type="text/javascript">
    $('#btn_help').on('click', function () {
        Home.help();
    });

    $('#btn_user_home').on('click' , function () {
        Home.userHome();
    });

    $('.btn_helper_feedback').on('click' , function () {
        Home.feedback($(this).attr('data-id'));
    });


    var initialSlide = $('.custom-slick-carousel').attr('data-initialSlide');
     $(".custom-slick-carousel.content-slider").bxSlider({
         controls: false,
         startSlide : initialSlide,
         pager : false
    });

     $('.btn_call_phone').on('click' , function (event) {

         Home.callPhone($(this).attr('data-phone'));
     });

    $('.volunteer_cancel').on('click', function (event) {
        var $this = $(this);
        AjaxHandler.delete('/api/volunteer/' + $this.attr('data-id'), function () {
            $("#remove_popup").modal('show');
        });
    });

    $('.btn_match_confirm').on('click', function (event) {
        AjaxHandler.put('/api/volunteer/' + $(this).attr('data-id') + '/accept', null, function () {
            $('#matched_popup').modal('show');
        });
    });

    $('.btn_match_no').on('click' , function (event) {
        AjaxHandler.put('/api/volunteer/' + $(this).attr('data-id') + '/reject', null, function () {
            $('#match_reject_popup').modal('show');
        });
    });

    $('.btn_arrive_yes').on('click' , function (event) {
       AjaxHandler.put('/api/volunteer/' + $(this).attr('data-id') + '/start', null, function () {
           $('#volunteer_start_popup').modal('show');
       });
    });

    $('.btn_volunteer_done').on('click' , function (event) {
        AjaxHandler.put('/api/volunteer/' + $(this).attr('data-id') + '/done', null, function () {
            $('#volunteer_done_popup').modal('show');
        });
    });



    $('#matched_popup').on('show.bs.modal', function (e) {
        setInterval(function () {
            $('#matched_popup').modal('hide');
        }, 3000);
    });

    $('#matched_popup').on('hidden.bs.modal', function (e) {
        location.reload();
    });


    $('#match_reject_popup').on('show.bs.modal', function (e) {
        setInterval(function () {
            $('#match_reject_popup').modal('hide');
        }, 3000);
    });

    $('#match_reject_popup').on('hidden.bs.modal', function (e) {
        location.reload();
    });

    $('#remove_popup').on('show.bs.modal', function (e) {
        setInterval(function () {
            $('#remove_popup').modal('hide');
        }, 3000);
    });

    $('#remove_popup').on('hidden.bs.modal', function (e) {
        location.reload();
    });
    $('#volunteer_start_popup').on('show.bs.modal', function (e) {
        setInterval(function () {
            $('#volunteer_start_popup').modal('hide');
        }, 3000);
    });

    $('#volunteer_start_popup').on('hidden.bs.modal', function (e) {
        location.reload();
    });

    $('#volunteer_done_popup').on('show.bs.modal', function (e) {
        setInterval(function () {
            $('#volunteer_done_popup').modal('hide');
        }, 3000);
    });

    $('#volunteer_done_popup').on('hidden.bs.modal', function (e) {
        location.reload();
    });


</script>

</body>
</html>
