    <!-- banner start -->
    <!-- ================ -->

        <div class="custom-slick-carousel content-slider" data-initialSlide="<%= centerIndex %>" style="background-color: rgba( 255, 255, 255, 0.5 );">
            <% for (var indexI = 0; indexI < result.volunteers.length; indexI++) {%>
                <div class="slide">
                    <div class="container">
                        <div class="row justify-content-center">

                            <div class="col-12">
                                <!-- name-and-slogan -->

                                <% if (result.volunteers[indexI].matchingStatus == 0) { %>
                                <div class="row standBy">
                                    <div class="col-sm-12">

                                        <div class="image-box style-2 mb-20 bordered light-gray-bg">
                                            <div class="overlay-container overlay-visible">
                                                <img src="/template/mobile/volunteer-search.jpg" alt="">
                                            </div>
                                            <div class="body">
                                                <h4 class="mb-10 text-info">
                                                    <i class="fa fa-tag  pr-1"></i>봉사 대기중
                                                </h4>
                                                <h4 class=" mb-10 text-default">
                                                    <i class="fa fa-calendar-o pr-1"></i> <%= moment(result.volunteers[indexI].startAt).format('MM-DD HH:mm') %>~ <%= moment(result.volunteers[indexI].endAt).format('MM-DD HH:mm') %>

                                                </h4>

                                                <div class="separator"></div>
                                                <h3><%= result.volunteers[indexI].content %></h3>
                                                <button  class="btn btn-danger btn-lg col-12 volunteer_cancel" data-id="<%= result.volunteers[indexI].volunteerId %>">취소<i class="fa fa-trash pl-1"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% } else if (result.volunteers[indexI].matchingStatus == 1) { %>

                                <div class="row">
                                    <div class="col-sm-12">

                                        <div class="image-box style-2 mb-20 bordered light-gray-bg">
                                            <div class="overlay-container overlay-visible">
                                                <img src="/photo/<%= result.volunteers[indexI].profileImage %>" alt="">
                                                <div class="overlay-bottom text-left">
                                                    <p class="lead margin-clear text-large font-weight-bold"><%= result.volunteers[indexI].name %>
                                                        님</p>
                                                </div>
                                            </div>
                                            <div class="body">
                                                <h4 class="mb-10 text-info">
                                                    <i class="fa fa-tag  pr-1"></i>봉사자 연결
                                                </h4>
                                                <h4 class=" mb-10 text-default"><i class="fa fa-calendar-o pr-1"></i> <%= moment(result.volunteers[indexI].startAt).format('MM-DD HH:mm') %>~ <%= moment(result.volunteers[indexI].endAt).format('MM-DD HH:mm') %></h4>
                                                <h4 class=" mb-10 text-default"><i class="fa fa-envelope pr-1"></i> <%= result.volunteers[indexI].content %>
                                                </h4>
                                                <div class="separator"></div>
                                                <h3><%= result.volunteers[indexI].name %>님에게 도움을 받으실래요?</h3>
                                                <div class="text-center">
                                                    <button class="btn_match_confirm btn btn-success btn-lg" data-id='<%= result.volunteers[indexI].volunteerId %>'>네<i class="fa"></i></button>
                                                    <button class="btn_match_no btn btn-danger btn-lg " data-id='<%= result.volunteers[indexI].volunteerId %>'>아니요<i class="fa"></i></button>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% } else if (result.volunteers[indexI].matchingStatus == 2 && result.volunteers[indexI].startStatus == 0) { %>
